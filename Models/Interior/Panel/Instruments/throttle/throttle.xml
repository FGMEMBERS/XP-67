    <center>
      <x-m> -0.24438 </x-m>
      <y-m>  0.00000 </y-m>
      <z-m> -0.01066 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>propellermanette2</object-name>
    <property>controls/engines/engine[1]/propeller-pitch</property>
