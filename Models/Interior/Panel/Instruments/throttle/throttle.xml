<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>throttle.ac</path>

  <animation>
    <object-name>base</object-name>
    <object-name>gearmanette</object-name>
    <object-name>throttlemanetteL</object-name>
    <object-name>throttlemanetteR</object-name>
    <object-name>mixturemanetteL</object-name>
    <object-name>mixturemanetteR</object-name>
    <object-name>propellermanette1</object-name>
    <object-name>propellermanette2</object-name>
    <object-name>propellermanette3</object-name>
    <object-name>propellermanette4</object-name>
    
  </animation>

  <!-- Manette des gazs -->
  <animation>
    <type>rotate</type>
    <object-name>throttlemanetteL</object-name>
    <property>controls/engines/engine[0]/throttle</property>
    <factor> -90 </factor>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>throttlemanetteR</object-name>
    <property>controls/engines/engine[1]/throttle</property>
    <factor> -90 </factor>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <!-- Manette de mixture -->
  <animation>
    <type>rotate</type>
    <object-name>mixturemanetteL</object-name>
    <property>controls/engines/engine[0]/mixture</property>
    <factor> -95 </factor>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>mixturemanetteR</object-name>
    <property>controls/engines/engine[1]/mixture</property>
    <factor> -95 </factor>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <!-- Manette de pas d'hÃ©lice -->
