    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <!-- Manette des flaps -->
  <animation>
    <type>rotate</type>
    <object-name>gearmanette</object-name>
    <property>gear/gear[0]/position-norm</property>
    <factor> 47 </factor>
    <center>
      <x-m> -0.02401 </x-m>
      <y-m>  0.01638 </y-m>
      <z-m> -0.08686 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <!-- Ajustage par molette
  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>throttlemanette</object-name>
    <action>
      <button>3</button>
      <binding>
        <command>property-adjust</command>
        <property>controls/engines/engine[0]/throttle</property>
        <step>0.1</step>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>throttlemanette</object-name>
